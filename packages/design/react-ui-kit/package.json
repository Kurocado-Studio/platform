{
  "name": "@kurocado-studio/react-ui-kit",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "private": true,
  "keywords": [],
  "author": "@kurocado-studio",
  "license": "MIT",
  "main": "./dist/exports.js",
  "module": "./dist/exports.js",
  "types": "./dist/exports.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.js"
    },
    "./styles.css": "./dist/exports.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ui-domain": "pnpm --filter @kurocado-studio/ui-kit-domain build",
    "build:typings": "api-extractor run --local",
    "build": "pnpm build:ui-domain && rm -rf dist && tsup && pnpm build:typings && rm -rf dist/exports.d.ts",
    "prepublishOnly": "node ../../../scripts/strip-workspaces.js .",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "rm -rf dist && tsup --watch",
    "dev:vite": "vite build --watch --mode development",
    "vite:watch": "vite build --watch --mode development",
    "build:vite": "rm -rf dist && vite build --mode production",
    "lint": "eslint --max-warnings=0 . --no-warn-ignored",
    "lint:fix": "eslint --max-warnings=0 . --fix --no-warn-ignored",
    "prettier:fix": "prettier --write . --ignore-path ../../../.prettierignore",
    "prettier:check": "prettier --check . --ignore-path ../../../.prettierignore",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@headlessui/react": "^2.2.4",
    "@headlessui/vue": "^1.7.23",
    "@kurocado-studio/html-forms": "1.0.0",
    "@kurocado-studio/ui-kit-domain": "workspace:*",
    "@react-aria/button": "^3.13.0",
    "@react-aria/textfield": "^3.17.3",
    "@tailwindcss/typography": "^0.5.16",
    "@vue/runtime-dom": "^3.5.14",
    "framer-motion": "^12.23.6",
    "lodash-es": "^4.17.21",
    "motion": "^12.11.0",
    "motion-v": "^1.0.2",
    "react": "19",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^2.1.0"
  },
  "devDependencies": {
    "@internal/config": "workspace:*",
    "@kurocado-studio/qa": "workspace:*",
    "@microsoft/api-extractor": "^7.52.11",
    "@storybook/react": "^8.6.13",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/compiler-sfc": "^3.5.14",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "semantic-release": "^24.2.3",
    "semantic-release-monorepo": "^8.0.2",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "unplugin-vue": "^7.0.1",
    "vite": "^5.2.8",
    "vite-node": "3.2.4",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vue": "^3.5.14",
    "vue-tsc": "^3.0.5"
  }
}
