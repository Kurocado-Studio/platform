{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "storybook-static/**"],
      "dependsOn": ["^internal:build", "^build"]
    },
    "internal:build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^internal:build"],
      "cache": false
    },
    "internal:build:domain": {
      "outputs": ["dist/**"],
      "dependsOn": ["^internal:build:domain"],
      "cache": false
    },
    "setup": {
      "dependsOn": ["^internal:build","^internal:build:domain", "^setup", "^build"],
      "cache": false
    },
    "internal": {
      "dependsOn": ["^internal"],
      "cache": false
    },
    "prettier:fix": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "prettier:check": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^setup", "^build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^setup", "^build"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^setup", "^build"],
      "cache": false
    },
    "lint:fix": {
      "dependsOn": ["^setup", "^build"],
      "cache": false
    },
    "dev:internal": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^internal:build", "^setup", "^build"],
      "cache": false,
      "persistent": true
    },
    "dev:storybook": {
      "dependsOn": ["^internal:build", "^setup", "^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "internal:preflight": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "internal:config": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "vite:watch": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "storybook": {
      "dependsOn": ["dev:watch"]
    },
    "preview-storybook": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
